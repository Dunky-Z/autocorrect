newline = ${ "\n" | "\r" }
space = ${ " "+ }

other = ${ !(string) ~ ANY }
comment = ${ single_line_comment }
single_line_comment = _{ "//" ~ (!(newline) ~ ANY)* }

string = ${ 
  ("@\"" ~ (!(newline | "\"") ~ ANY)* ~ "\"") 
}
line = _{ string | comment | space | other | newline }
item = _{ SOI ~ line* ~ EOI } 